IF문 
점수에 따라 A ~ F의 등급을 준다.
결과물 
점수 : 00점
등급 : A~F

<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
</head>	
<body>
<script>
    var score = prompt("점수를 입력하세요.");

    if (score<0 || score>100) {
        document.write("점수가 잘못 입력되었습니다.");
    }
    else if (score >= 95) {
        var grade = "A+";
        document.write("점수 : " + score + "<br>" + "등급 : " + grade);
    }
    else if (score >= 90) {
        var grade = "A";
        document.write("점수 : " + score + "<br>" + "등급 : " + grade);
    }
    else if (score >= 85) {
        var grade = "B+";
        document.write("점수 : " + score + "<br>" + "등급 : " + grade);
    }
    else if (score >= 80) {
        var grade = "B";
        document.write("점수 : " + score + "<br>" + "등급 : " + grade);
    }
    else if (score >= 75) {
        var grade = "C+";
        document.write("점수 : " + score + "<br>" + "등급 : " + grade);
    }
    else if (score >= 70) {
        var grade = "C";
        document.write("점수 : " + score + "<br>" + "등급 : " + grade);
    }
    else if (score >= 65) {
        var grade = "D+";
        document.write("점수 : " + score + "<br>" + "등급 : " + grade);
    }
    else if (score >= 60) {
        var grade = "D";
        document.write("점수 : " + score + "<br>" + "등급 : " + grade);
    }
    else {
        var grade = "F";
        document.write("점수 : " + score + "<br>" + "등급 : " + grade);
    }

</script>
</body>  
</html>
